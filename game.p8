--region global vars
glyphs={"\139","\145","\148","\131","\142","\151"}
left=0 right=1 up=2 down=3 fire1=4 fire2=5
tile = 8
scene = 0
score = 0
screenwidth = 127
screenheight = 127
player = {}
player.animindex = 1
player.play = "idle"
player.x = 32
player.y = 120
player.w = 7
player.h = 7
player.lives = 1
player.dead = false
player.invincible = false
player.speed = tile
player.lastHit = 0
player.lastFrame = 0
player.lastInput = 0
--
anims = {                      --all character animations, described by name
	idle = { fr = 0.5, 1, 2 }, --idle has a frame rate of 15, and loops between sprite #1 and #2 forever
	walkright = { fr = 0.5, next = "idle", 3, 4 }, --you can reuse frames of art to save space. Walking 4,3,4,5,4,3,4,5,...
	walkleft = { fr = 0.5, next = "idle", 1, 2 }, --you can reuse frames of art to save space. Walking 4,3,4,5,4,3,4,5,...
}
-- rivers
rivers = { { crocodiles = { }, rafts = {} }, { crocodiles = { }, rafts = {} }, { crocodiles = { }, rafts = {} } }
water = { 7, 8, 9, 10, 11, 12, 13, 14, 15 }
raft = 17
crocodile = { mouth = 26, head = 27, body = { 28, 29 }, tail = 30, endtail=31 }
rafts = { }
walls = { 18, 19, 20 }
floor = 21
mice = 5
hearth = 6

--endregion

-- game loop
function _init()
	-- must initialise the cart data with a "key" before we can use it
	cartdata("scoreboard_fondation")
	high_score_table.load_scores()
	--high_score_table.save_scores()
	-- generate 3 rivers
	for r = 0, 2 do
		-- create river
		for x = 0, screenwidth, tile do
			yS = (r * 40) + 16
			for y = yS, yS + 16, tile do
				if x == 0 or x == 120 then
					add(rivers[r+1], { x = x, y = y, sprt = water[1] })
				else
					add(rivers[r+1], { x = x, y = y, sprt = water[flr(rnd(8)) + 1] })
				end
			end
		end
		-- create entities for 3 lines in each river
		for i = 0, 2 do
			river = rivers[r+1]
			sy = (i*8+r*8) + 16*(r+1) + 16 * r
			-- each crocodile has a random speed, based on the river
			-- the speed is calculated by the distance from the top of the river
			-- the closer to the top, the slower the crocodile
			-- the closer to the bottom, the faster the crocodile
			-- the first river is the fastest, the last river is the slowest
			spd = 0.5 + (r * 0.1) + (i * 0.1) + rnd(3) * 0.1
			printh("spd:" .. spd.. " r:"..r.." i:"..i)
			-- crocodiles
			add(river.crocodiles, {
				x = 127, y = sy,
				speed = spd, w = 8, h = 8,
				sprt = { crocodile.mouth, crocodile.head, crocodile.body[flr(rnd(2)) + 1], crocodile.tail, crocodile.endtail }
			})
			-- rafts
			add(river.rafts, { x = 127 - tile, y = sy, sprt = raft, speed = spd, w = 8, h = 8 })
		end
	end

	music(0) -- title music
end

function _update60()
	if scene == 0 then
		titleupdate()
	elseif scene == 1 then
		gameupdate()
	elseif scene == 2 then
		endupdate()
	end
	-- kill player if have 0 score
	if score < 0 then
		player.invincible = false
		player.dead = true
		player.lives = 0
		playerHit()
	end
end

function _draw()
	cls()
	palt(0, true)
	if scene == 0 then
		titledraw()
	elseif scene == 1 then
		gamedraw()
	elseif scene == 2 then
		enddraw()
	elseif scene == 3 then
		highscoredraw()
	end
	for b=0,6 do
		if btn(b,0) then
			print(glyphs[b+1], b*8 + 0, 120, 1)
		end
	end
end

--region update functions
function titleupdate()
	if btnp(fire1) then
		scene = 1
	elseif btnp(fire2) then
		scene = 2
	end
end

function gameupdate()
	-- score: number of tiles reached + lives * 100 - time passed
	score = (120*8) - (player.y*8) + (player.lives * 100) - flr(time())
	crocodileUpdate()
	playerControl()
	playerUpdate()
	wincondition()
end

function crocodileUpdate()
	for r = 1, 3 do
		for i = 1, #rivers[r].crocodiles do
			-- after the crocodile reaches the left side of the screen, it will reappear on the right side
			rivers[r].crocodiles[i].x -= rivers[r].crocodiles[i].speed
			if rivers[r].crocodiles[i].x < -32 then
				rivers[r].crocodiles[i].x = 127
				-- random speed for passing crocodiles
				rivers[r].crocodiles[i].speed = 0.5 + (r * 0.1) + (i * 0.1) + rnd(3) * 0.1
			end
			-- check if player is on a crocodile
			if player.y == rivers[r].crocodiles[i].y and player.x >= rivers[r].crocodiles[i].x and player.x <= rivers[r].crocodiles[i].x + 40 then
				player.x -= rivers[r].crocodiles[i].speed
			end
			if iscolliding(player, rivers[r].crocodiles[i]) then
				playerHit();
			end
			--at random, every 5sec play croc roar
			if time() % 5 > 4.95 then
				sfx(5)
			end
		end
		for i = 1, #rivers[r].rafts do
			rivers[r].rafts[i].x -= rivers[r].rafts[i].speed
			if rivers[r].rafts[i].x < -32 then
				rivers[r].rafts[i].x = 127
			end
			-- check if player is on a raft
			if iscolliding(player, rivers[r].rafts[i]) then
				player.x -= rivers[r].rafts[i].speed
			end
		end
	end
end

function endupdate()
	high_score_table.update()
	high_score_table.check_current_score(high_score_table.current_score)
	if btnp(fire1) then
		-- reset
		scene = 1
		player.x = 63
		player.y = 127
		player.lives = 3
		player.invincible = false
		player.dead = false
		player.lastHit = 0
		player.lastFrame = 0
		player.lastInput = 0
		score = 0
		high_score_table.current_score = 0
	end
end
--endregion

--region draw functions
function titledraw()
	local titletxt = "gator crossing"
	local starttxt = "press \151 to start"
	local scoretxt = "press \142 for highscores"
	rectfill(0, 0, screenwidth, screenheight, 1)
	print(titletxt, hcenter(titletxt), screenheight / 4, 10)
	print(starttxt, hcenter(starttxt), (screenheight / 4) + (screenheight / 2), 7)
	print(scoretxt, hcenter(scoretxt), (screenheight / 4) + (screenheight / 2) + 10, 7)
end

function gamedraw()
	map(0, 0, 0, 0, 128, 32)
	print(score, 1, 1, 7)
	-- draw player lives
	for i = 1, player.lives do
		print(chr(135), flr((#tostr(score) * 4) / 2) + ((i+1) * 8), 1, 7)
	end
	riverDraw()
	playerdDraw()
end

function enddraw()
	high_score_table.draw()
end

function playerdDraw()
	if player.invincible then
		-- flash player sprite
		if time() % 0.1 > 0.05 then
			spr(1, player.x, player.y)
		end
		return
	end
	-- draw player sprite
	spr(anims[player.play][player.animindex], player.x, player.y)
end

function riverDraw()
	for r = 1, 3 do
		for i = 1, #rivers[r] do
			spr(rivers[r][i].sprt, rivers[r][i].x, rivers[r][i].y)
			for j = 1, #rivers[r].crocodiles do
				for k = 1, #rivers[r].crocodiles[j].sprt do
					spr(rivers[r].crocodiles[j].sprt[k], rivers[r].crocodiles[j].x + 8 * k, rivers[r].crocodiles[j].y)
				end
			end
			for j = 1, #rivers[r].rafts do
				spr(rivers[r].rafts[j].sprt, rivers[r].rafts[j].x, rivers[r].rafts[j].y)
			end
		end
	end
end
--endregion

-- handle button inputs
function playerControl()
	-- debouce player input so that the player moves 1 tile every 0.25s at max
	if (player.lastInput > 0) then
		if time() - player.lastInput < 0.25 then
			return
		end
	end

	pX = flr(player.x / 8)
	pY = flr(player.y / 8)
	-- handle player movement
	if (btn(left)) then
		-- check if the next tile is a wall
		ntile = mget(pX - 1, pY)
		if ntile != walls[1] and ntile != walls[2] and ntile != walls[3] then
			player.x -= player.speed
			player.play = "walkleft"
		end
	end
	if (btn(right)) then
		ntile = mget(pX + 1, pY)
		if ntile != walls[1] and ntile != walls[2] and ntile != walls[3] then
			player.x += player.speed
			player.play = "walkright"
		end
	end
	if (btn(up)) then
		ntile = mget(pX, pY - 1)
		if ntile != walls[1] and ntile != walls[2] and ntile != walls[3] then
			player.y -= player.speed
		end
	end
	if (btn(down)) then
		ntile = mget(pX, pY + 1)
		if ntile != walls[1] and ntile != walls[2] and ntile != walls[3] then
			player.y += player.speed
		end
	end

	-- check if the player is still onscreen
	if (player.x <= 0) then
		player.x = 0
	end
	if (player.x >= screenwidth - player.w) then
		player.x = screenwidth - player.h
	end
	if (player.y <= 0) then
		player.y = 0
	end
	if (player.y >= screenheight - player.h) then
		player.y = screenheight - player.h
	end

	player.lastInput = time()
end

-- check if the player is on a crocodile
function playerUpdate()
	if player.lastHit > 0 then
		if time() - player.lastHit > 1 then
			player.invincible = false
		end
	end

	-- animate player
	if time() - player.lastFrame > anims[player.play].fr then
		player.animindex = player.animindex + 1
		if player.animindex > 2 then
			player.animindex = 1
		end
		player.lastFrame = time()
	end
end

function playerHit()
	if player.invincible then
		if time() - player.lastHit > 1 then
			player.invincible = false
			player.lastHit = time()
		end
		return
	end

	sfx(4) -- hit sfx
	player.invincible = true
	player.lastHit = time()
	-- reset player position
	player.y = 127
	player.x = 32
	player.lives = player.lives - 1

	-- end game
	if player.lives == 0 then
		sfx(4) -- hit sfx
		player.dead = true
		scene = 2
		high_score_table.add_current_score(score)
	end
end

